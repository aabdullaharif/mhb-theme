    "d636d5ec-4aea-445b-af04-9aace2927d10": {
      "type": "multi-column",
      "blocks": {
        "template--14450086608958__d636d5ec-4aea-445b-af04-9aace2927d10-16668167469184e4c5-0": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/icon-repair-broken.png",
            "image_width": 100,
            "image_border": "rgba(0,0,0,0)",
            "title": "",
            "content": "<p>Snag a chain or bend a bridge cuff? We repair broken Betsy &amp; Iya fashion jewelry for free with only a few limitations.<\/p>",
            "extra_emphasis": false,
            "link_text": "",
            "link_url": "",
            "link_style": "link",
